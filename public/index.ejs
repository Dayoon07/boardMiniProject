<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
</head>
<body>
    <header style="display: flex; justify-content: space-between; align-items: center;">
        <div></div>
        <div>
            <a href="/login">로그인</a>
            <a href="/signup">회원가입</a>
        </div>
    </header>
    <%= true ? "참" : "거짓" %> - <%= false ? "참" : "거짓" %>

    <% users.forEach(user => { %>
        <li><%= user.id %> - <%= user.name %> - <%= user.age %></li>
    <% }); %>
    <br>
    <a href="/upload">업로드하러가기</a>
    <% files.forEach(e => { %>
        <div>
            <a href="/imgid?id=<%= e.id %>"><%= e.id %></a>
            <span><%= e.filename %> - <%= e.createAt %></span>
        </div>
    <% }); %>
</body>
</html>
